[nodeservers]
mean-local-auth ansible_ssh_host=YOUR_SERVER_IP_HERE ansible_ssh_private_key_file=~/.ssh/mean-local-auth

[redisservers]
mean-local-auth ansible_ssh_host=YOUR_SERVER_IP_HERE ansible_ssh_private_key_file=~/.ssh/mean-local-auth

[mongoservers]
mean-local-auth ansible_ssh_host=YOUR_SERVER_IP_HERE ansible_ssh_private_key_file=~/.ssh/mean-local-auth

[nginxservers]
mean-local-auth ansible_ssh_host=YOUR_SERVER_IP_HERE ansible_ssh_private_key_file=~/.ssh/mean-local-auth
